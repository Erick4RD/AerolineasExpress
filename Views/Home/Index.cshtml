@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome to AeroExpres</h1>

</div>

<div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
    </ol>
    <div class="carousel-inner">

        <div class="carousel-item active">
            <img class="d-block w-100" src="  https://www.aeropuertoquito.aero/images/share_quiport.jpg"/>
        </div>

        <div class="carousel-item">
            <img class="d-block w-100" src="https://www.10wallpaper.com/wallpaper/1366x768/1310/towards_the_sunset-Aircraft_HD_Wallpaper_1366x768.jpg" />
        </div>

        <div class="carousel-item">

            <img class="d-block w-100" src="https://c4.wallpaperflare.com/wallpaper/867/739/683/amanecer-avion-despegando-wallpaper-preview.jpg" />

        </div>

        <div class="carousel-item">

            <img class="d-block w-100" src="https://www.10wallpaper.com/wallpaper/1366x768/1112/private_plane-civil_aviation_aircraft_Desktop_Wallpapers_1366x768.jpg" />
                 
        </div>

        <div class="carousel-item">

            <img class="d-block w-100" src="https://i.pinimg.com/originals/c1/b7/05/c1b7050b0543293b0ea22fd655214476.jpg" />

        </div>

    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
